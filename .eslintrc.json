{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "valid-jsdoc":"off",
        "require-jsdoc": "off",
        "quotes":"off",
        "max-len":"off",
        "linebreak-style":"off"
    }
}
